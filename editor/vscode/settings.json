{
    "editor.fontFamily": "Monaspace Neon, CommitMono, Jetbrains Mono, Fira Code, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.lineHeight": 24,
    "terminal.integrated.fontSize": 15,
    "window.restoreWindows": "none",
    "workbench.settings.editor": "json",
    "window.commandCenter": true,
    "workbench.editor.showTabs": "none",
    "editor.smoothScrolling": true,
    "terminal.integrated.macOptionIsMeta": true,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.mouseWheelScrollSensitivity": 4,
    "explorer.autoReveal": false,
    "breadcrumbs.enabled": false,
    "files.simpleDialog.enable": true,
    "workbench.editor.editorActionsLocation": "hidden",
    "editor.wordWrap": "on",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "outline.icons": false,
    "workbench.editor.enablePreview": false,
    "editor.bracketPairColorization.enabled": false,
    "editor.stickyScroll.enabled": false,
    "git.decorations.enabled": false,
    "workbench.tips.enabled": false,
    "editor.minimap.enabled": false,
    "workbench.editor.wrapTabs": true,
    "diffEditor.renderSideBySide": false,
    "explorer.confirmPasteNative": false,
    "workbench.navigationControl.enabled": false,
    "http.proxy": "http://localhost:2081",
    "[go]": { "editor.formatOnSave": true },
    "go.addTags": { "options": "" },
    "go.toolsManagement.autoUpdate": true,
    "git.confirmSync": false,
    "scm.diffDecorations": "none",
    "remote.autoForwardPortsSource": "hybrid",
    "explorer.confirmDelete": false,
    "vim.leader": " ",
    "vim.useSystemClipboard": true,
    "vim.timeout": 200,
    "vim.insertModeKeyBindings": [ {"before": ["j", "k"], "after": ["<esc>"]}, {"before": ["k", "j"], "after": ["<esc>"]}],
    "workbench.sideBar.location": "right",
}
