[
  // Editor Layout
  { "key": "cmd+1", "command": "workbench.action.editorLayoutSingle", },
  { "key": "cmd+2", "command": "workbench.action.editorLayoutTwoColumns", },
  { "key": "cmd+3", "command": "workbench.action.editorLayoutThreeColumns", },
  { "key": "cmd+[", "command": "workbench.action.focusPreviousGroup" },
  { "key": "cmd+]", "command": "workbench.action.focusNextGroup" },
  // Next/Prev Error
  { "key": "alt+[", "command": "editor.action.marker.prev" },
  { "key": "alt+]", "command": "editor.action.marker.next" },
  // Run Task
  { "key": "cmd+b", "command": "workbench.action.tasks.reRunTask", },
  { "key": "cmd+shift+b", "command": "workbench.action.tasks.runTask" },
  // Open new window
  { "key": "cmd+shift+o", "command": "workbench.action.files.openFileFolderInNewWindow" },
  // Toggle Explorer
  { "key": "cmd+k cmd+e", "command": "workbench.view.explorer", "when": "!explorerViewletVisible" },
  { "key": "cmd+k cmd+e", "command": "workbench.action.toggleSidebarVisibility", "when": "explorerViewletVisible" },
  // Toggle Terminal
  { "key": "cmd+k cmd+t", "command": "workbench.action.terminal.toggleTerminal" },
  // Toggle Search
  { "key": "cmd+k cmd+s", "command": "workbench.view.search", "when": "!activeViewlet == 'workbench.view.search'" },
  { "key": "cmd+k cmd+s", "command": "workbench.action.toggleSidebarVisibility", "when": "activeViewlet == 'workbench.view.search'" },
  { "key": "cmd+shift+f", "command": "workbench.action.toggleSidebarVisibility", "when": "view.workbench.view.search.visible" },
  // Toggle Git
  { "key": "cmd+k cmd+g", "command": "workbench.view.scm", "when": "!view.workbench.scm.visible" },
  { "key": "cmd+k cmd+g", "command": "workbench.action.toggleSidebarVisibility", "when": "view.workbench.scm.visible" },

  { "key": "cmd+enter", "command": "editor.action.quickFix", "when": "editorHasCodeActionsProvider && textInputFocus && !editorReadonly" },
  { "key": "cmd+ctrl+r", "command": "editor.action.rename", "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly" },
  { "key": "cmd+.", "command": "editor.action.revealDefinition", "when": "editorHasDefinitionProvider && editorTextFocus" },
  { "key": "cmd+,", "command": "workbench.action.navigateBack" },
  { "key": "cmd+shift+.", "command": "editor.action.goToReferences" },
  // Consisten Ctrl n / p
  { "key": "ctrl+n", "command": "selectNextSuggestion", "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion" },
  { "key": "ctrl+p", "command": "selectPrevSuggestion", "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion" },
  { "key": "ctrl+n", "command": "workbench.action.quickOpenSelectNext", "when": "inQuickOpen" },
  { "key": "ctrl+p", "command": "workbench.action.quickOpenSelectPrevious", "when": "inQuickOpen" },
]
