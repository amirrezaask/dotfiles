[
  // Font Size
  { "key": "ctrl+-", "command": "editor.action.fontZoomOut" },
  { "key": "ctrl+=", "command": "editor.action.fontZoomIn" },

  { "key": "ctrl+k ctrl+k", "command": "workbench.panel.chat" },
  { "key": "cmd+k cmd+k", "command": "workbench.panel.chat" },
  
  // Next/Prev Error
  { "key": "alt+[", "command": "editor.action.marker.prev" },
  { "key": "alt+]", "command": "editor.action.marker.next" },
  { "key": "cmd+[", "command": "editor.action.marker.prev" },
  { "key": "cmd+]", "command": "editor.action.marker.next" },
  
  // Consisten Ctrl n / p
  { "key": "ctrl+n", "command": "selectNextSuggestion", "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion" },
  { "key": "ctrl+p", "command": "selectPrevSuggestion", "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion" },
  { "key": "ctrl+n", "command": "workbench.action.quickOpenSelectNext", "when": "inQuickOpen" },
  { "key": "ctrl+p", "command": "workbench.action.quickOpenSelectPrevious", "when": "inQuickOpen" },

  // Open new window
  { "key": "cmd+shift+o", "command": "workbench.action.files.openFileFolderInNewWindow", },
  { "key": "ctrl+shift+o", "command": "workbench.action.files.openFileFolderInNewWindow", },

  // Open in current window
  { "key": "cmd+o", "command": "workbench.action.files.openFileFolder", },
  { "key": "ctrl+o", "command": "workbench.action.files.openFileFolder", },

  // Toggle Explorer
  { "key": "cmd+shift+e", "command": "workbench.view.explorer",                  "when": "!view.workbench.explorer.fileView.visible" },
  { "key": "cmd+shift+e", "command": "workbench.action.toggleSidebarVisibility", "when": "view.workbench.explorer.fileView.visible" },
  { "key": "ctrl+shift+e", "command": "workbench.view.explorer",                  "when": "!view.workbench.explorer.fileView.visible" },
  { "key": "ctrl+shift+e", "command": "workbench.action.toggleSidebarVisibility", "when": "view.workbench.explorer.fileView.visible" },
  
  // Toggle Terminal
  { "key": "cmd+j", "command": "workbench.action.terminal.toggleTerminal" },
  { "key": "ctrl+j", "command": "workbench.action.terminal.toggleTerminal" },
  
  // Toggle Git
  { "key": "cmd+shift+g", "command": "workbench.view.scm", "when": "!view.workbench.scm.visible" },
  { "key": "cmd+shift+g", "command": "workbench.action.toggleSidebarVisibility", "when": "view.workbench.scm.visible" },
  { "key": "ctrl+shift+g", "command": "workbench.view.scm", "when": "!view.workbench.scm.visible" },
  { "key": "ctrl+shift+g", "command": "workbench.action.toggleSidebarVisibility", "when": "view.workbench.scm.visible" },

  // Quick Fix
  { "key": "cmd+enter", "command": "editor.action.quickFix", "when": "editorHasCodeActionsProvider && textInputFocus && !editorReadonly" },
  { "key": "ctrl+enter", "command": "editor.action.quickFix", "when": "editorHasCodeActionsProvider && textInputFocus && !editorReadonly" },
  
  // Rename
  { "key": "cmd+shift+r", "command": "editor.action.rename", "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly" },
  { "key": "ctrl+shift+r", "command": "editor.action.rename", "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly" },
  
  // Goto Definition
  { "key": "cmd+.", "command": "editor.action.revealDefinition", "when": "editorHasDefinitionProvider && editorTextFocus" },
  { "key": "cmd+,", "command": "workbench.action.navigateBack" },
  { "key": "alt+.", "command": "editor.action.revealDefinition", "when": "editorHasDefinitionProvider && editorTextFocus" },
  { "key": "alt+,", "command": "workbench.action.navigateBack" },
  { "key": "cmd+shift+.", "command": "editor.action.goToReferences" },
  { "key": "alt+shift+.", "command": "editor.action.goToReferences" },



]
