[
    // Editor Layout
    { "key": "ctrl+shift+1", "command": "workbench.action.editorLayoutSingle", },
    { "key": "ctrl+shift+2", "command": "workbench.action.editorLayoutTwoColumns", },
    { "key": "ctrl+shift+3", "command": "workbench.action.editorLayoutThreeColumns", },
    { "key": "ctrl+shift+[", "command": "workbench.action.focusPreviousGroup" },
    { "key": "ctrl+shift+]", "command": "workbench.action.focusNextGroup" },
    // Next/Prev Error
    { "key": "alt+[", "command": "editor.action.marker.prev" },
    { "key": "alt+]", "command": "editor.action.marker.next" },
    // Run Task
    { "key": "ctrl+shift+b", "command": "workbench.action.tasks.reRunTask", },
    { "key": "ctrl+shift+b", "command": "workbench.action.tasks.runTask" },
    // Open new window
    { "key": "ctrl+shift+o", "command": "workbench.action.files.openFileFolderInNewWindow" },
    // Toggle Explorer
    { "key": "ctrl+k ctrl+e", "command": "workbench.view.explorer", "when": "!explorerViewletVisible" },
    { "key": "ctrl+k ctrl+e", "command": "workbench.action.toggleSidebarVisibility", "when": "explorerViewletVisible" },
    // Toggle Terminal
    { "key": "ctrl+k ctrl+t", "command": "workbench.action.terminal.toggleTerminal" },
    // Toggle Search
    { "key": "ctrl+shift+f", "command": "workbench.action.toggleSidebarVisibility", "when": "view.workbench.view.search.visible" },
    // Toggle Git
    { "key": "ctrl+shift+g", "command": "workbench.view.scm", "when": "!view.workbench.scm.visible" },
    { "key": "ctrl+shift+g", "command": "workbench.action.toggleSidebarVisibility", "when": "view.workbench.scm.visible" },
  
    { "key": "ctrl+shift+enter", "command": "editor.action.quickFix", "when": "editorHasCodeActionsProvider && textInputFocus && !editorReadonly" },
    { "key": "ctrl+shift+r", "command": "editor.action.rename", "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly" },
    { "key": "ctrl+shift+.", "command": "editor.action.revealDefinition", "when": "editorHasDefinitionProvider && editorTextFocus" },
    { "key": "ctrl+shift+,", "command": "workbench.action.navigateBack" },
    { "key": "ctrl+shift+.", "command": "editor.action.goToReferences" },
    // Consisten Ctrl n / p
    { "key": "ctrl+n", "command": "selectNextSuggestion", "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion" },
    { "key": "ctrl+p", "command": "selectPrevSuggestion", "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion" },
    { "key": "ctrl+n", "command": "workbench.action.quickOpenSelectNext", "when": "inQuickOpen" },
    { "key": "ctrl+p", "command": "workbench.action.quickOpenSelectPrevious", "when": "inQuickOpen" },
  ]
  