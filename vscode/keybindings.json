  { "key": "ctrl+r", "when": "editorTextFocus", "command": "editor.action.goToReferences" }, // Find all references ( Windows / Linux )
  { "key": "ctrl+.", "when": "editorTextFocus", "command": "editor.action.revealDefinition" }, // Goto Definition ( Windows / Linux )
  { "key": "ctrl+,", "when": "editorTextFocus", "command": "workbench.action.navigateBack" }, // Jump back where you were ( Windows / Linux )
  { "key": "ctrl+shift+a", "command": "editor.action.quickFix" }, // Quick fix ( Windows / Linux )
  { "key": "ctrl+shift+r", "command": "editor.action.rename" }, // Rename ( Windows / Linux )
  { "key": "ctrl+1", "command": "workbench.view.explorer" }, // Toggle File explorer
  { "key": "ctrl+2", "command": "workbench.files.action.collapseExplorerFolders"}, // collapse all folders in explorer view
