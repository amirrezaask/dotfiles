{
  "vim.useSystemClipboard": true,
  "vim.foldfix": true,
  "vim.leader": " ",
  "vim.insertModeKeyBindings": [
    { "before": [ "j", "k" ], "after": [ "<esc>" ] },
    { "before": [ "k", "j" ], "after": [ "<esc>" ] } ],
  "vim.normalModeKeyBindings": [
    { "before": [ "F" ], "commands": [ "editor.action.formatDocument" ] },
    { "before": [ "C" ], "commands": [ "editor.action.quickFix" ] },
    { "before": [ "<C-d>" ], "after": [ "<C-d>", "z", "z" ] },
    { "before": [ "<C-u>" ], "after": [ "<C-u>", "z", "z" ] },
    { "before": [ "[", "d" ], "commands": [ "editor.action.marker.prev" ] },
    { "before": [ "]", "d" ], "commands": [ "editor.action.marker.next" ] },
    { "before": [ "g", "r" ], "commands": [ "editor.action.goToReferences" ] },
    { "before": [ "g", "d" ], "commands": [ "editor.action.revealDefinition" ] },
    { "before": [ "g", "i" ], "commands": [ "editor.action.goToImplementation" ] },
    { "before": [ "R" ], "commands": [ "editor.action.rename" ] },
    { "before": [ "K" ], "commands": [ "editor.action.showHover" ] },
    { "before": [ "j" ], "after": [ "g", "j" ] },
    { "before": [ "k" ], "after": [ "g", "k" ] },
    { "before": [ "<C-l>" ], "after": [ "<C-w>", "l" ] },
    { "before": [ "<C-k>" ], "after": [ "<C-w>", "k" ] },
    { "before": [ "<C-j>" ], "after": [ "<C-w>", "j" ] },
    { "before": [ "<C-h>" ], "after": [ "<C-w>", "h" ] },
    { "before": [ "<leader>", "p" ], "commands": [ "workbench.action.quickOpen" ] },
    { "before": [ "<leader>", "P" ], "commands": [ "workbench.action.showCommands" ] },
    { "before": [ "<leader>", "o" ], "commands": [ "workbench.action.gotoSymbol" ] },
    { "before": [ "<leader>", "O" ], "commands": [ "workbench.action.showAllSymbols" ] },
    { "before": [ "<leader>", "h" ], "commands": [ "workbench.action.splitEditorDown" ] },
    { "before": [ "<leader>", "v" ], "commands": [ "workbench.action.splitEditorRight" ] },
    { "before": [ "=" ], "commands": [ "editor.action.smartSelect.expand" ] },
    { "before": [ "-" ], "commands": [ "editor.action.smartSelect.shrink" ] }
  ],
  "window.title": "${folderPath}",
  "window.dialogStyle": "custom",
  "editor.fontFamily": "Jetbrains Mono",
  "editor.fontSize": 14,
  "editor.renderWhitespace": "selection",
  "editor.lineNumbers": "on",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.emptyWindow": true,
  "workbench.editor.untitled.hint": "hidden",
  "editor.lightbulb.enabled": false,
  "workbench.editor.enablePreview": false,
  "workbench.tips.enabled": false,
  "extensions.ignoreRecommendations": true,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.fontLigatures": true,
  "editor.accessibilitySupport": "off",
  "window.commandCenter": false,
  "editor.inlayHints.enabled": "off",
  "editor.minimap.enabled": false,
  "editor.bracketPairColorization.enabled": false,
  "git.openRepositoryInParentFolders": "never",
  "go.useLanguageServer": true,
  "[jsonc]": { "editor.defaultFormatter": "vscode.json-language-features" },
  "json.format.keepLines": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "[rust]": { "editor.formatOnSave": true, "editor.tabSize": 4 },
  "[ocaml]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[go]": { "editor.formatOnSave": true },
  "[yaml]": { "editor.guides.indentation": true },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "[python]": { "editor.formatOnType": true },
  "lldb.suppressUpdateNotifications": true,
  "editor.cursorBlinking": "solid",
  "workbench.panel.opensMaximized": "always",
  "workbench.panel.defaultLocation": "bottom",
  "workbench.startupEditor": "none",
  "telemetry.telemetryLevel": "off",
  "go.toolsManagement.autoUpdate": true,
  "rest-client.showResponseInDifferentTab": true,
  "rest-client.requestNameAsResponseTabTitle": true,
  "rest-client.environmentVariables": {
    "production": { },
    "beta": {
      "jwtToken": "{{$shared jwtToken}}",
      "baseURL": "http://beta.rahavard365.com"
    },
