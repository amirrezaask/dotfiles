{
  "workbench.preferredDarkColorTheme": "Visual Studio Dark",
  "workbench.preferredLightColorTheme": "Visual Studio Light",
  "window.autoDetectColorScheme": true,
  "window.title": "${folderPath}",
  "window.dialogStyle": "custom",
  "editor.fontSize": 14,
  "editor.renderWhitespace": "selection",
  "editor.lineNumbers": "on",
  "workbench.activityBar.visible": false,
  "editor.wordWrap": "on",
  "workbench.editor.untitled.hint": "hidden",
  "editor.lightbulb.enabled": false,
  "workbench.editor.enablePreview": false,
  "workbench.tips.enabled": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.accessibilitySupport": "off",
  "editor.inlayHints.enabled": "off",
  "editor.minimap.enabled": false,
  "editor.bracketPairColorization.enabled": false,
  "git.openRepositoryInParentFolders": "never",
  "go.useLanguageServer": true,
  "[jsonc]": { "editor.defaultFormatter": "vscode.json-language-features" },
  "json.format.keepLines": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "[rust]": { "editor.formatOnSave": true, "editor.tabSize": 4 },
  "[go]": { "editor.formatOnSave": true },
  "[yaml]": { "editor.guides.indentation": true },
  "workbench.startupEditor": "none",
  "go.toolsManagement.autoUpdate": true,
  "rest-client.showResponseInDifferentTab": true,
  "rest-client.requestNameAsResponseTabTitle": true,
