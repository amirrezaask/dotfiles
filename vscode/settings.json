{
  "workbench.preferredDarkColorTheme": "Visual Studio Dark",
  "workbench.preferredLightColorTheme": "Visual Studio Light",
  "extensions.ignoreRecommendations": true,
  "window.autoDetectColorScheme": true,
  "window.title": "${folderPath}",
  "window.dialogStyle": "custom",
  "editor.fontSize": 14,
  "editor.wordWrap": "on",
  "workbench.settings.editor": "json",
  "workbench.layoutControl.enabled": false,
  "workbench.editor.untitled.hint": "hidden",
  "editor.lightbulb.enabled": false,
  "workbench.editor.enablePreview": false,
  "workbench.tips.enabled": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.accessibilitySupport": "off",
  "editor.inlayHints.enabled": "off",
  "editor.minimap.enabled": false,
  "editor.bracketPairColorization.enabled": false,
  "git.openRepositoryInParentFolders": "never",
  "[jsonc]": { "editor.defaultFormatter": "vscode.json-language-features" },
  "json.format.keepLines": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "[rust]": { "editor.formatOnSave": true, "editor.tabSize": 4 },
  "[go]": { "editor.formatOnSave": true },
  "[yaml]": { "editor.guides.indentation": true },
  "workbench.startupEditor": "none",
  "go.toolsManagement.autoUpdate": true,
  "rest-client.showResponseInDifferentTab": true,
  "rest-client.requestNameAsResponseTabTitle": true,
  "gitProjectManager.baseProjectsFolders": [ "~/w", "~/dev" ],
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "workbench.colorTheme": "Visual Studio Dark",
  "window.zoomLevel": 1
}
// christian-kohler.path-intellisense
// felipecaputo.git-project-manager
// golang.go
// humao.rest-client
// ocamllabs.ocaml-platform
// qcz.text-power-tools
// redhat.vscode-yaml
// rust-lang.rust-analyzer
// usernamehw.errorlens
// ziglang.vscode-zig
// zxh404.vscode-proto3
