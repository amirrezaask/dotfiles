#!/usr/bin/bash
filename=`echo $samfile`

extension=`echo "${filename##*.}"`

if [ $extension == 'go' ]; then
	cmd='go build -v ./...'
fi
eval $cmd
exit_status=$?

if [[ $exit_status -ne 0 ]]; then
	echo "============ Build Failed ============"
else
	echo "============ Build Succeded ============"
fi