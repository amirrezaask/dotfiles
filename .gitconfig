[pull]
	rebase = true

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[alias]
	fa = fetch --all
	ft = fetch --tags
	st = status
	co = checkout
	re = rebase
	s = status 
