{
  "editor.fontFamily": "JetBrains Mono, Maple Mono",
  "editor.fontWeight": "500",
  "editor.fontSize": 14,
  "editor.lineHeight": 1.5,
  "editor.cursorBlinking": "solid",
  "files.autoSave": "onFocusChange",
  "editor.wordWrap": "on",
  "editor.minimap.enabled": false,
  "editor.mouseWheelScrollSensitivity": 2,
  "window.restoreWindows": "none",
  "workbench.settings.editor": "json",
  "terminal.integrated.macOptionIsMeta": true,
  "workbench.editor.enablePreview": false,
  "files.simpleDialog.enable": true,
  "[go]": { "editor.formatOnSave": true },
  "explorer.confirmDelete": false,
  "workbench.startupEditor": "none",
  "editor.bracketPairColorization.enabled": false,
  "gitProjectManager.baseProjectsFolders": ["~/dev" ],
  "gitProjectManager.openInNewWindow": true,
  "git.blame.statusBarItem.enabled": true,
  "terminal.integrated.fontSize": 16,
  "window.autoDetectColorScheme": true,
  "extensions.ignoreRecommendations": true,
  "go.toolsManagement.autoUpdate": true,
  "update.mode": "silentlyApplyOnQuit",
  "window.commandCenter": false,
  "chat.commandCenter.enabled": false,
  "workbench.layoutControl.enabled": false,
  "workbench.preferredLightColorTheme": "Default Dark Modern",
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.iconTheme": "symbols",
  "kilo-code.debug": false,
  "kilo-code.allowedCommands": [
    "git log",
    "git diff",
    "git show"
  ],
  "kilo-code.deniedCommands": [],
  "workbench.editor.showTabs": "single",
  "redhat.telemetry.enabled": true,
}
