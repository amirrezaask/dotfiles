{
  "editor.fontFamily": "Jetbrains Mono, Fira Code",
  "editor.fontWeight": "bold",
  "editor.fontSize": 14,
  "editor.cursorBlinking": "solid",
  "window.restoreWindows": "none",
  "workbench.settings.editor": "json",
  "terminal.integrated.macOptionIsMeta": true,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.mouseWheelScrollSensitivity": 4,
  "files.simpleDialog.enable": true,
  "editor.wordWrap": "on",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "workbench.editor.enablePreview": false,
  "editor.bracketPairColorization.enabled": false,
  "editor.stickyScroll.enabled": false,
  "git.decorations.enabled": false,
  "workbench.tips.enabled": false,
  "editor.minimap.enabled": false,
  "workbench.editor.wrapTabs": true,
  "diffEditor.renderSideBySide": false,
  "explorer.confirmPasteNative": false,
  "workbench.navigationControl.enabled": false,
  "http.proxy": "http://localhost:2081",
  "[go]": { "editor.formatOnSave": true },
  "[odin]": {"editor.formatOnSave": true },
  "go.addTags": { "options": "" },
  "go.toolsManagement.autoUpdate": true,
  "git.confirmSync": false,
  "scm.diffDecorations": "none",
  "remote.autoForwardPortsSource": "hybrid",
  "explorer.confirmDelete": false,
  "simpleBrowser.focusLockIndicator.enabled": false,
  "git.openRepositoryInParentFolders": "never",
  "extensions.ignoreRecommendations": true,
  "cursor.cpp.enablePartialAccepts": true,
  "workbench.editor.showTabs": "none",
  "workbench.statusBar.visible": false,
  "breadcrumbs.enabled": false,
}
