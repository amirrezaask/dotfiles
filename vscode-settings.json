{
  // "http.proxy": "http://localhost:1081",
  "editor.fontSize": 14,
  "editor.fontFamily": "Fira Code",
  "workbench.editor.editorActionsLocation": "hidden",
  "editor.wordWrap": "on",
  "editor.minimap.enabled": false,
  "diffEditor.renderSideBySide": false,
  "editor.mouseWheelScrollSensitivity": 2,
  "window.restoreWindows": "none",
  "workbench.settings.editor": "json",
  "terminal.integrated.macOptionIsMeta": true,
  "files.simpleDialog.enable": true,
  "[go]": { "editor.formatOnSave": true },
  "git.confirmSync": false,
  "git.openRepositoryInParentFolders": "never",
  "workbench.activityBar.location": "top",
  "explorer.confirmDelete": false,
  "editor.stickyScroll.enabled": false,
  "workbench.layoutControl.enabled": false,
  "breadcrumbs.enabled": false,
  "window.commandCenter": false,
  "workbench.startupEditor": "none",
  "window.customTitleBarVisibility": "windowed",
  "go.toolsManagement.autoUpdate": true,
  "vim.leader": " ",
  "vim.useSystemClipboard": true,
  "vim.timeout": 200,
  "vim.normalModeKeyBindings": [
    { "before": ["<C-d>"], "after": ["<C-d>", "z", "z"] },
    { "before": ["<C-u>"], "after": ["<C-u>", "z", "z"] },
    { "before": [ "g", "r" ], "commands": [ "references-view.findReferences" ], },
    { "before": [ "g", "l" ], "commands": [ "editor.action.showHover" ], },
    { "before": [ "K" ], "commands": [ "editor.action.showHover" ], },
    { "before": [ "[", "[" ], "commands": [ "editor.action.marker.prev" ], },
    { "before": [ "]", "]" ], "commands": [ "editor.action.marker.next" ], },
    { "before": [ "C"], "commands": ["editor.action.quickFix"] },
    { "before": [ "R" ], "commands": [ "editor.action.rename" ] },
    { "before": ["{"], "commands": [ "references-view.prev"] },
    { "before": ["}"], "commands": [ "references-view.next"] },
    { "before": ["<c-q>"], "commands": ["references-view.tree.focus"] },
    { "before": ["j"], "after": ["g", "j"]},
    { "before": ["k"], "after": ["g", "k"]}
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<esc>"
      ],
    },
    {
      "before": [
        "k",
        "j"
      ],
      "after": [
        "<esc>"
      ],
    },
  ],
}
